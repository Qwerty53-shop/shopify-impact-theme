    "custom_liquid_eCycGW": {
      "type": "custom-liquid",
      "settings": {
        "full_width": true,
        "remove_vertical_spacing": false,
        "remove_horizontal_spacing": false,
        "liquid": "<style>\n  /* Section globale du carrousel */\n  .video-carousel-container {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    gap: 15px;\n    margin-top: 30px;\n    overflow-x: scroll;\n    padding: 10px 0;\n  }\n\n  .video-carousel-container::-webkit-scrollbar {\n    display: none; /* Masquer la scrollbar */\n  }\n\n  /* Style des cercles */\n  .video-thumbnail {\n    width: 80px;\n    height: 80px;\n    border-radius: 50%;\n    overflow: hidden;\n    border: 2px solid #ff6a00; /* Couleur ressemblant à Instagram */\n    cursor: pointer;\n    transition: all 0.3s ease-in-out;\n  }\n\n  .video-thumbnail:hover {\n    transform: scale(1.1);\n    border-color: #ff4081; /* Changement de couleur lors du survol */\n  }\n\n  .video-thumbnail img {\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n  }\n\n  /* Style pour la vidéo affichée en grand */\n  .video-popup {\n    display: none;\n    position: fixed;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    background-color: rgba(0, 0, 0, 0.7);\n    justify-content: center;\n    align-items: center;\n    z-index: 1000;\n  }\n\n  .video-popup.active {\n    display: flex;\n  }\n\n  .popup-video-container {\n    position: relative;\n    width: 80%;\n    max-width: 800px;\n    background-color: #fff;\n    padding: 20px;\n    border-radius: 15px;\n  }\n\n  .popup-video-container video {\n    width: 100%;\n    height: auto;\n    border-radius: 15px;\n  }\n\n  .close-btn {\n    position: absolute;\n    top: 10px;\n    right: 10px;\n    font-size: 24px;\n    color: #fff;\n    background-color: #ff4081;\n    border: none;\n    border-radius: 50%;\n    padding: 10px;\n    cursor: pointer;\n  }\n\n  .close-btn:hover {\n    background-color: #ff6a00;\n  }\n</style>\n\n<!-- Conteneur du carrousel de vidéos -->\n<div class=\"video-carousel-container\">\n  <!-- Vidéo 1 -->\n  <div class=\"video-thumbnail\" data-video-url=\"https://www.youtube.com/watch?v=V9PARh0kW6A&t=75s\">\n    <img src=\"https://cdn.shopify.com/videos/c/o/v/0aab38d4141748ae89e6838e44048ad3.mp4\" alt=\"Story Video 1\">\n  </div>\n\n  <!-- Vidéo 2 -->\n  <div class=\"video-thumbnail\" data-video-url=\"https://cdn.shopify.com/videos/c/o/v/0aab38d4141748ae89e6838e44048ad3.mp4\">\n    <img src=\"https://cdn.shopify.com/videos/c/o/v/0aab38d4141748ae89e6838e44048ad3.mp4\" alt=\"Story Video 2\">\n  </div>\n\n  <!-- Vidéo 3 -->\n  <div class=\"video-thumbnail\" data-video-url=\"https://cdn.shopify.com/videos/c/o/v/0aab38d4141748ae89e6838e44048ad3.mp4\">\n    <img src=\"https://cdn.shopify.com/videos/c/o/v/0aab38d4141748ae89e6838e44048ad3.mp4\" alt=\"Story Video 3\">\n  </div>\n\n  <!-- Vidéo 4 -->\n  <div class=\"video-thumbnail\" data-video-url=\"https://cdn.shopify.com/videos/c/o/v/0aab38d4141748ae89e6838e44048ad3.mp4\">\n    <img src=\"https://cdn.shopify.com/videos/c/o/v/0aab38d4141748ae89e6838e44048ad3.mp4\" alt=\"Story Video 4\">\n  </div>\n</div>\n\n<!-- Pop-up vidéo (Affichage en grand) -->\n<div class=\"video-popup\">\n  <div class=\"popup-video-container\">\n    <video id=\"popup-video\" controls>\n      <source src=\"\" type=\"video/mp4\">\n      Your browser does not support the video tag.\n    </video>\n    <button class=\"close-btn\">X</button>\n  </div>\n</div>\n\n<script>\n  // Script pour afficher la vidéo en grand\n  const videoThumbnails = document.querySelectorAll('.video-thumbnail');\n  const videoPopup = document.querySelector('.video-popup');\n  const popupVideo = document.getElementById('popup-video');\n  const closeButton = document.querySelector('.close-btn');\n\n  videoThumbnails.forEach(thumbnail => {\n    thumbnail.addEventListener('click', function() {\n      const videoUrl = this.getAttribute('data-video-url');\n      popupVideo.src = videoUrl;  // Charge la vidéo\n      videoPopup.classList.add('active');  // Affiche la pop-up\n      popupVideo.play();  // Lance la lecture de la vidéo\n    });\n  });\n\n  closeButton.addEventListener('click', function() {\n    videoPopup.classList.remove('active');  // Masque la pop-up\n    popupVideo.pause();  // Pause la vidéo\n    popupVideo.src = '';  // Arrête la vidéo\n  });\n</script>",
