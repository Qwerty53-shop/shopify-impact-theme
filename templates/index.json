            "liquid": "<style>\n  .timer-container {\n    background: #fffff;\n    padding: 0px 0;\n    display: flex;\n    justify-content: center;\n    font-family: Arial, sans-serif;\n  }\n\n  .timer-wrapper {\n    display: flex;\n    align-items: flex-start;\n    gap: 0px;\n  }\n\n  .time-column {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    width: 60px;\n    margin: 0 5px;\n  }\n\n  .time-value {\n    font-size: 36px;\n    font-weight: bold;\n    color: #1a1a1a;\n    letter-spacing: 1px;\n    line-height: 1;\n  }\n\n  .time-label {\n    font-size: 12px;\n    color: #666;\n    margin-top: 5px;\n  }\n\n  .separator {\n    font-size: 36px;\n    font-weight: bold;\n    color: #1a1a1a;\n    margin: 0 5px;\n    line-height: 1;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n  }\n</style>\n\n<div class=\"timer-container\">\n  <div class=\"timer-wrapper\">\n    <div class=\"time-column\">\n      <div class=\"time-value\" id=\"days\">00</div>\n      <div class=\"time-label\">Jours</div>\n    </div>\n    <div class=\"separator\">:</div>\n    <div class=\"time-column\">\n      <div class=\"time-value\" id=\"hours\">00</div>\n      <div class=\"time-label\">Hrs</div>\n    </div>\n    <div class=\"separator\">:</div>\n    <div class=\"time-column\">\n      <div class=\"time-value\" id=\"minutes\">10</div>\n      <div class=\"time-label\">Mins</div>\n    </div>\n    <div class=\"separator\">:</div>\n    <div class=\"time-column\">\n      <div class=\"time-value\" id=\"seconds\">23</div>\n      <div class=\"time-label\">Secs</div>\n    </div>\n  </div>\n</div>\n\n<script>\n  function startCountdown(duration) {\n    let timer = duration;\n\n    const daysEl = document.getElementById('days');\n    const hoursEl = document.getElementById('hours');\n    const minutesEl = document.getElementById('minutes');\n    const secondsEl = document.getElementById('seconds');\n\n    const interval = setInterval(() => {\n      const days = Math.floor(timer / 86400);\n      const hours = Math.floor((timer % 86400) / 3600);\n      const minutes = Math.floor((timer % 3600) / 60);\n      const seconds = timer % 60;\n\n      daysEl.textContent = String(days).padStart(2, '0');\n      hoursEl.textContent = String(hours).padStart(2, '0');\n      minutesEl.textContent = String(minutes).padStart(2, '0');\n      secondsEl.textContent = String(seconds).padStart(2, '0');\n\n      if (--timer < 0) {\n        clearInterval(interval);\n        daysEl.textContent = '00';\n        hoursEl.textContent = '00';\n        minutesEl.textContent = '00';\n        secondsEl.textContent = '00';\n      }\n    }, 1000);\n  }\n\n  window.addEventListener('DOMContentLoaded', () => {\n    const totalSeconds = 10 * 60 + 23;\n    startCountdown(totalSeconds);\n  });\n</script>"
          }
